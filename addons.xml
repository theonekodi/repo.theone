<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.theone.wizard" name="The One Wizard" version="1.0" provider-name="Marco Nogueira">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">The One Wizard</summary>
    <description lang="en">The One Wizard allows you to get access to the latest The One Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.marco.nogueira" name="Marco Nogueira Wizard" version="2.0" provider-name="Marco Nogueira">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
		<summary lang="en">Bem Vindo ao Marco Nogueira Wizard</summary>
		<description lang="en">Este softwere Ã© feito para instalar a minhas biulds</description>
		<disclaimer lang="en">For help and support ... WWW.kodihelp.COM</disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="repo.theone" name="The one repository" version="1.0" provider-name="Marco Nogueira">
	<extension point="xbmc.addon.repository" name="The one repository">
		<info compressed="false">https://github.com/theonekodi/repo.theone/blob/master/addons.xml</info>
		<checksum>https://github.com/theonekodi/repo.theone/blob/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/theonekodi/repo.theone/blob/master/repo.theone/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Addons</summary>
		<description>best fou you</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
